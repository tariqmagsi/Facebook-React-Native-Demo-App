{
  "expo": {
    "name": "Tariq's Facebook",
    "slug": "myapp",
    "privacy": "public",
    "sdkVersion": "34.0.0",
    "platforms": ["ios", "android", "web"],
    "android": {
      "package": "com.tariq.facebook"
    },
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/fb.ico",
    "splash": {
      "image": "./assets/fb.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    }
  }
}
